{
    "manifest_version": 2,
  
    "name": "Simple Dictionary Plugin",
    "description": "This extension will look up words using the Pearson Dictionary API",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html",
     "default_script": "popup.js"
    },
    "permissions": [
     "activeTab",
     "contextMenus"
     ]
  }